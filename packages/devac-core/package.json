{
  "name": "@pietgk/devac-core",
  "version": "0.1.1",
  "description": "DevAC core analysis engine - DuckDB/Parquet-based code graph",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgk/vivief.git",
    "directory": "packages/devac-core"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./parsers": {
      "types": "./dist/parsers/index.d.ts",
      "import": "./dist/parsers/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit -p tsconfig.test.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@babel/parser": "^7.26.0",
    "@babel/traverse": "^7.26.0",
    "@babel/types": "^7.26.0",
    "chokidar": "^3.6.0",
    "duckdb-async": "^1.1.3",
    "glob": "^11.0.0",
    "ts-morph": "^24.0.0",
    "tree-sitter": "^0.22.4",
    "tree-sitter-c-sharp": "^0.23.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.28",
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
