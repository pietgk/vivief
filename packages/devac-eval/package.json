{
  "name": "@pietgk/devac-eval",
  "version": "1.2.0",
  "description": "Answer quality evaluation framework for DevAC",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "devac-eval": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "prebuild": "node ../../scripts/generate-version.mjs .",
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "pretest": "ps -eo ppid,pid,command 2>/dev/null | grep vitest | awk '$1 == 1 { print $2 }' | xargs kill 2>/dev/null || true",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@pietgk/devac-core": "workspace:*",
    "commander": "^12.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.28",
    "typescript": "^5.7.2",
    "vitest": "^3.1.1"
  },
  "files": ["dist"],
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgk/vivief.git",
    "directory": "packages/devac-eval"
  },
  "keywords": ["devac", "evaluation", "llm", "quality", "benchmark"],
  "license": "MIT"
}
